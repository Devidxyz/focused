{
  "name": "focused",
  "description": "Improve focus and increase productivity by blocking distracting websites while working or studying.",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack --watch --mode development",
    "build": "rimraf build && webpack --mode production",
    "lint": "eslint . --ext .ts --fix --max-warnings=0",
    "test:unit": "jest ./src",
    "test:unit:watch": "yarn test:unit --watch",
    "test:e2e": "jest -c test/config/jest.config.json --runInBand ",
    "test:e2e:watch": "yarn test:e2e --watch",
    "test": "yarn test:unit && yarn test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimimikadze/focused"
  },
  "keywords": [
    "focused",
    "chrome",
    "extension"
  ],
  "author": "Dimi Mikadze",
  "bugs": {
    "url": "https://github.com/dimimikadze/focused/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/chrome": "^0.0.110",
    "@types/jest": "^26.0.9",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-jest": "^26.2.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^4.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.0",
    "jest": "^26.2.2",
    "jest-puppeteer": "^4.4.0",
    "mini-css-extract-plugin": "^0.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "puppeteer": "^5.3.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "build"
  ],
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test"
    }
  },
  "license": "MIT",
  "dependencies": {}
}
